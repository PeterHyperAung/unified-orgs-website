---
import orgs from "../../resources/organizations.json";
import OrganizationsList from "../../components/OrganizationsList.astro";
import Layout from "../../layouts/Layout.astro";
const categories = [...new Set(orgs.map((org) => org.field))];
---

<Layout title="United Orgs">
  <main>
    <OrganizationsList client:load orgs={orgs} fields={categories} />
  </main>
</Layout>
